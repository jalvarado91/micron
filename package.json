{
  "name": "@yotieapp/micron",
  "version": "1.6.3",
  "description": "A Micro route handler utility",
  "source": "src/index.js",
  "main": "dist/index.modern.js",
  "module": "dist/index.modern.module.js",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "microbundle src/index.js -f cjs,es,modern --target node --compress",
    "dev": "microbundle --target node watch",
    "test": "jest",
    "prepublishOnly": "echo '//npm.pkg.github.com/:_authToken=${NPM_TOKEN}' >> .npmrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yotieapp/micron.git"
  },
  "author": "Ashley Narcisse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yotieapp/micron/issues"
  },
  "homepage": "https://github.com/yotieapp/micron#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@yotieapp/utils": "^1.7.5",
    "micro": "^9.3.4"
  },
  "devDependencies": {
    "microbundle": "next"
  }
}
